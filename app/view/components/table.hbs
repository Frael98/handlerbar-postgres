<div class="col-sm-6 animation-keyframes-demo">

    <table class="table table-dark table-striped table-hover m-2">
        <thead>
            <tr>
                <th>#</th>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Estado Civil</th>
                <th> Editar </th>
                <th> Eliminar </th>
            </tr>
        </thead>
        <tbody>
            {{#each usuarios}}
            <tr>
                {{!-- <td>{{ @key }}</td> --}}
                <td>{{id_usuario}}</td>
                <td>{{nombre}}</td>
                <td> {{ apellido }}</td>
                <td> {{ estado_civil }}</td>
                {{!-- <i class="fa-duotone fa-pen-to-square fa-bounce"></i> --}}
                <td class="text-center editar"><i class="fa-regular fa-pen-to-square " style="color: #07a02d;"></i></td>
                <td class="text-center eliminar"><i class="fa-solid fa-trash" style="color: #d30909;"></i></td>
            </tr>
            {{/each}}
        </tbody>
    </table>


</div>